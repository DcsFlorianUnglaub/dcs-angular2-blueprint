    "ts-helpers": "1.1.1",
    "ts-node": "1.3.0",
    "tslint": "3.15.1",
    "typedoc": "0.4.5",
    "typescript": "2.0.2",
    "typings": "1.3.3",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.21",
